target 'CJWBaseObject_OC' do
   pod 'AFNetworking'
   pod 'SDWebImage'  ,'< 4.3.0'#此处限制版本是因为PYPhotoBrowser不支持高版本的SD，后续支持的话可不做
   pod 'Masonry'
   pod 'MJRefresh'
   pod 'YYKit'
   pod 'JKCategories' #iOS类别库
   pod 'EasyNavigation' #自定义导航栏
   pod 'TZImagePickerController'#仿微信图片选择器
   pod 'PYPhotoBrowser'#图片浏览器
   pod 'MBProgressHUD'
   pod 'UITableView+FDTemplateLayoutCell'
#   pod 'WMPageController'
   pod 'SDCycleScrollView' #banner库
   pod 'TPKeyboardAvoiding'#防止键盘遮挡
   pod 'ReactiveObjC'
  pod 'AliyunOSSiOS' #阿里云OSS
   pod 'AlipaySDK-iOS' #支付宝支付SDK
   pod 'WechatOpenSDK' #微信支付SDK
   pod 'JXCategoryView'
   pod 'JXPagingView/Pager'
# 友盟
pod 'UMCCommon'
#    pod 'UMCSecurityPlugins' (会和支付宝SDK冲突)
pod 'UMCAnalytics' #友盟统计
# U-Share SDK UI模块（分享面板，建议添加）
pod 'UMCShare/UI'
# 集成微信(精简版0.2M) (备注:添加完成版的会和微信支付冲突)
pod 'UMCShare/Social/ReducedWeChat'
#    # 集成微信(完整版14.4M)
#    pod 'UMCShare/Social/WeChat'
# 集成QQ/QZone/TIM(完整版7.6M)
pod 'UMCShare/Social/QQ'
# 集成新浪微博(完整版25.3M)
pod 'UMCShare/Social/Sina'

    # pod 'FGiOSKit', '0.0.2'
    
